{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strict": true,
        "outDir": "./dist",
        "typeRoots": ["./node_modules/@types/", "**/types/**/*.d.ts"],
        "baseUrl": "./src/",
        "allowSyntheticDefaultImports": true,
        "lib": ["es2015", "es2016", "es2017.object"]
    },
    "files": ["./src/list.tsx"]
}
